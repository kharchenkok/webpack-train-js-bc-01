{{#each this}}
<li class="post-feed__item">
    <article class="post">
        <header class="post__header">
            <img src="{{author.avatar}}" alt="{{author.name}}" height="80" class="post__avatar">
            <div class="post__meta">
                <h2 class="post__author">{{author.name}}</h2>
                <p class="post__date">{{creatAt}}</p>
            </div>
        </header>
        <p class="post__text"></p>
        <footer class="post__footer">
            <ul class="tag__list">
                {{#each tags}}
                <li class="tag-list__item">
                    <a class="tag-list__link" href="#">{{this}}</a>
                </li>
                {{/each}}
            </ul>
        </footer>
    </article>
</li>
{{/each}}